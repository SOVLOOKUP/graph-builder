{
  "name": "graph-builder",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently -n parcel,vite \"parcel watch src/worker.ts --dist-dir public/ --no-source-maps \" \"vite\"",
    "build": "parcel build src/worker.ts --dist-dir public/ --no-source-maps && vue-tsc --noEmit && vite build",
    "serve": "vite preview"
  },
  "dependencies": {
    "@antv/x6": "^1.29.1",
    "@mdi/font": "^6.5.95",
    "acebase": "^1.12.4",
    "threads": "^1.7.0",
    "vue": "^3.2.16",
    "vue-router": "4",
    "vuetify": "^3.0.0-alpha.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.4",
    "@vitejs/plugin-vue": "^2.0.1",
    "@vuetify/vite-plugin": "^1.0.0-alpha.10",
    "concurrently": "^6.5.1",
    "parcel": "^2.0.1",
    "sass": "^1.45.1",
    "typescript": "^4.5.4",
    "vite": "^2.7.6",
    "vue-tsc": "^0.30.0"
  }
}
